<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ssh.entity.BooksEntity" table="books" schema="test">
        <id name="id" column="id"/>
        <property name="author" column="author"/>
        <property name="name" column="name"/>
        <property name="price" column="price"/>
        <property name="pubDate" column="pubDate"/>
        <property name="categoryId" column="category_id"/>
        <many-to-one name="categoryEntity" column="category_id" class="ssh.entity.CategoryEntity"></many-to-one>
    </class>
</hibernate-mapping>